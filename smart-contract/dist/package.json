        "start": "fabric-chaincode-node start"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "fabric-client": "^1.4.0",
        "fabric-contract-api": "^1.4.1",
        "fabric-network": "^1.4.0",
        "fabric-shim": "^1.4.0",
        "indicative": "^5.0.8",
        "js-yaml": "^3.12.2",
        "typescript": "^3.3.3333"
    },
    "devDependencies": {
        "@types/jest": "^24.0.0",
        "@types/js-yaml": "^3.12.0",
        "@types/node": "^10.12.23",
        "ts-node": "^8.0.2",
        "tslint": "^5.12.1"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
